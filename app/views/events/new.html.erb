<div class="container">
 <h1>Create a new event</h1>
  <%= simple_form_for(@event) do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :date %>
    <%= f.input :time %>
    <%= f.input :photo, as: :file %>
    <%= f.submit class: 'btn btn-success' %>
  <% end %>
</div>
